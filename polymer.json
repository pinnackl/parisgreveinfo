{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/strike-info.html",
    "src/strike-trafic.html",
    "src/strike-404.html",
    "src/pinnackl-notification.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "src/config.js",
    "manifest.json",
    "firebase-messaging-sw.js",
    "bower_components/app-storage/app-indexeddb-mirror/app-indexeddb-mirror-worker.js",
    "bower_components/app-storage/app-indexeddb-mirror/common-worker-scope.js",
    "images/gifs/*",
    "images/ratp/*"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    }
  ]
}
